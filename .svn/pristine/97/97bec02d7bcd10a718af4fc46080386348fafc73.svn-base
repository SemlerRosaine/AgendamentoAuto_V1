/**
 * Metodos que atualizar os dados dentro da tabela sendo que o primeiro somente carrega e o segundo recebe uma lista como parametro
 */
    public void atualizarGridAgendados() {
        this.atualizarGridAgendados(new AgendamentoDao().carregaTodos());
    }

    private void atualizarGridAgendados(List<Agendamento> agendamentos) {
        this.agendamentos = agendamentos;
        this.jTAgendados.removeAll();
        this.tbAgendados.setRowCount(0);
        for (Agendamento agendamento : agendamentos) {
            tbAgendados.addRow(agendamento);
        }
    }
    /**
     * Metodo que realiza a construção da tabela dentro do JTable
     * @return 
     */

    private MyTableModel<Agendamento> construirTableModel() {
        this.tbAgendados = new MyTableModel<Agendamento>() {
            @Override
            public void addRow(Agendamento objeto) {
                this.addRow(new String[]{
                            objeto.getDataAgendamento().toString(),
                            objeto.getCliente(),
                            objeto.getTelefone()
                        });
            }

            @Override
            public boolean isCellEditable(int i, int i1) {
                return false;
            }
        };
        this.tbAgendados.setColumnIdentifiers(new String[]{
                    "Data",
                    "Nome/Razão social",
                    "Telefone"
                });
        return this.tbAgendados;
    }
