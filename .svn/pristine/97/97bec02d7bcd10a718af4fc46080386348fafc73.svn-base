    private void pesquisaAgenda() {
        agendamentos = new ArrayList<Agendamento>();
        Date dataAgenda;
        DateFormat dataFormatada = new SimpleDateFormat("dd/MM/yyyy");
        try {
            dataAgenda = dataFormatada.parse(txtDataAgendada.getText());
            buscarAgenda(dataAgenda);
            if(agendamentos.size() > 0){
                atualizarGridAgendados();
            }
            
        } catch (ParseException ex) {
        }

    }

